<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.psit_project">
    <uses-permission android:name="android.permission.INTERNET">
    </uses-permission>
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="PSIT"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PSIT_PROJECT">
        <activity
            android:name="com.example.psit_project.MainSearch"
            android:exported="true" />
        <activity
            android:name="com.example.psit_project.Department"
            android:exported="true" />
        <activity
            android:name="com.example.psit_project.Administration"
            android:exported="true" />
        <activity
            android:name="com.example.psit_project.Login"
            android:exported="true" />
        <activity
            android:name="com.example.psit_project.UpdateData"
            android:exported="true" />
        <activity
            android:name="com.example.psit_project.Add_new"
            android:exported="true" />
        <activity
            android:name="com.example.psit_project.Modify"
            android:exported="true" />
        <activity
            android:name="com.example.psit_project.SplashActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.psit_project.MainActivity"
            android:exported="true" />
    </application>

</manifest>